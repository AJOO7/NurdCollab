<script>
  // loaclising room id for use in application
  const ROOM_ID = "<%= roomId %>";
  // providng searching facilities to user through alert
  function findInfo() {
    alert(
      "Ctrl-F / Cmd-F -> Start searching \nCtrl-G / Cmd-G -> Find next \nShift-Ctrl-G / Shift-Cmd-G -> Find previous \nShift-Ctrl-F / Cmd-Option-F -> Replace \nShift-Ctrl-R / Shift-Cmd-Option-F -> Replace all \nAlt-F -> Persistent search \nAlt-G -> Jump to line "
    );
  }
  // increasing size of text in the editor
  function incSize() {
    curSize = parseInt($(".CodeMirror").css("font-size")) + 2;
    if (curSize <= 32) $(".CodeMirror").css("font-size", curSize);
  }
  // decreasing size of text in the editor
  function decSize() {
    curSize = parseInt($(".CodeMirror").css("font-size")) - 2;
    if (curSize >= 14) $(".CodeMirror").css("font-size", curSize);
  }
  // copying  id in form of temp space
  function copyClipboard(element) {
    var $temp = $("<input>");
    $("body").append($temp);
    $temp.val($(element).html()).select();
    document.execCommand("copy");
    $temp.remove();
    // alert("working");
  }
</script>
