<div class="editor-container">
  <div class="editor-main" style="margin:25px">
    <div
      class="room-container"
      style="display: flex; flex-direction: row; height: 20px"
    >
      <p>
        Room ID&nbsp&nbsp:
        <button
          onclick="copyClipboard('#room')"
          style="margin-bottom: 0; padding: 0"
          title="Click to Copy Room ID"
          class="btn"
        >
          <div id="room"><%= roomId %></div>
        </button>
      </p>

      <!--    <p style="margin-bottom: 0; padding: 0">Copy</p>  -->
    </div>
    <br />
    <div class="utility-container">
      <div class="left">
        <button id="cpy" onclick="copyToClipboard()" title="Copy Code">
          <i class="fas fa-copy fa-2x"></i>
        </button>
        <!--try replacing with copy fontawesome -->
        <button id="download" title="Download code">
          <i class="fas fa-file-download fa-2x"></i>
        </button>
        <button
          id="uploadButton"
          title="Upload"
          style="background-color: transparent; border: none"
        >
          <i class="fas fa-file-upload fa-2x"> </i>
        </button>
        <input id="fileInput" type="file" style="height: 0; width: 0" />
        <button id="clr" onclick="reset()" title="Clear code">
          <i class="fas fa-broom fa-2x"></i>
        </button>
        <button id="clr" onclick="findInfo()" title="Search code">
          <i class="fas fa-search fa-2x"></i>
        </button>
        <button id="clr" onclick="incSize()">
          <i class="fas fa-plus fa-2x"></i>
        </button>
        <button id="clr" onclick="decSize()">
          <i class="fas fa-minus fa-2x"></i>
